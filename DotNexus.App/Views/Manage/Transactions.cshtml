@model DotNexus.App.Models.ManageTransactionsViewModel

@{
    ViewData["Title"] = "Transactions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Head {
    <link rel="stylesheet" href="~/css/manage.transactions.css" />
}

<div class="container-fluid mt-4">
    <h1>Transactions</h1>

    @{ await Html.RenderPartialAsync("_GenesisTransactions"); }
</div>

@section Scripts {
    <script src="~/js/manage.transactions.js"></script>
    <script type="text/javascript">
        window.vm = new dotnexus.ManageTransactions({
            txs: @Json.Serialize(Model.Transactions)
        });
    </script>
}